{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": true,
    "sourceMap": true,
    "target": "ES2018",
    "lib": ["ES2019.Object", "es2018", "DOM"],
    "strict": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false,
    "noUncheckedIndexedAccess": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["jest", "node"]
  }
}
